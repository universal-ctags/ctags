%auxil "struct parserCtx *"

%earlysource {
    #include "general.h"
}

%header { struct parserCtx; }

%source {
    #include "kotlin_pre.h"
}

file <-
    ("#!" [^\n\r]* _* NL+)? NL* (
        (
            "@"
            / (
                Hidden
                / NL
            ) "@"
        ) "file" !(
            Letter
            / UnicodeDigit
        ) NL* ":" _* NL* (
            "[" _* (
                userType __* valueArguments
                / userType
            )+ _* "]"
            / userType __* valueArguments
            / userType
        ) _* NL*
    )* _* (
        "package" !(
            Letter
            / UnicodeDigit
        ) { PUSH_KIND(auxil, K_PACKAGE); } _ <simpleIdentifier (__* "." simpleIdentifier)*> { makeKotlinTag(auxil, $1, $1s, true); } _* semi?
    )* _* (
        "import" !(
