BSREQU		<-   '>>>='    Spacing
COLON		<-   ':'       Spacing
COMMA		<-   ',' {
		    eatComma(auxil, $0s);
		}	       Spacing
DEC		<-   '--'      Spacing
DIV		<-   '/' !'='  Spacing
DIVEQU		<-   '/='      Spacing
DOT		<-   '.'       Spacing
ELLIPSIS	<-   '...'     Spacing
EQU		<-   '=' !'='  Spacing
EQUAL		<-   '=='      Spacing
GE		<-   '>='      Spacing
GT		<-   '>'![=>]  Spacing
HAT		<-   '^' !'='	Spacing
HATEQU		<-   '^='      Spacing
