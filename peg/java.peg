LBRK		<-   '['       Spacing
LE		<-   '<='      Spacing
LPAR		<-   '('       Spacing
LPOINT		<-   '<'       Spacing
LT		<-   '<' ![=<]	Spacing
LWING		<-   '{'       Spacing
MINUS		<-   '-' ![=\-] Spacing
MINUSEQU	<-   '-='      Spacing
MOD		<-   '%' !'='	Spacing
MODEQU		<-   '%='      Spacing
NOTEQUAL	<-   '!='      Spacing
OR		<-   '|' ![=|]	Spacing
OREQU		<-   '|='      Spacing
OROR		<-   '||'      Spacing
PLUS		<-   '+' ![=+]	Spacing
PLUSEQU		<-   '+='      Spacing
QUERY		<-   '?'       Spacing
RBRK		<-   ']'       Spacing
RPAR		<-   ')'       Spacing
RPOINT		<-   '>'       Spacing
RWING		<-   '}' {
		    eatRightwing(auxil, $0s);
		}	       Spacing
SEMI		<-   ';'       Spacing
SL		<-   '<<' !'='	Spacing
SLEQU		<-   '<<='     Spacing
